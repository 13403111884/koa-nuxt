{
  "name": "ts-nuxt",
  "version": "1.0.0",
  "description": "My stylish Nuxt.js project",
  "author": "liuyi",
  "private": true,
  "scripts": {
    "dev": "tsc && cross-env NODE_ENV=development nodemon server/index.ts --watch server",
