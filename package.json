    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@types/koa": "^2.11.2",
    "@types/node": "^13.9.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
